class Solution {
    public String reverseWords(String s) {
        
        s=s.trim();
        String ans[]=s.split("\\s+");
        
        StringBuilder str=new StringBuilder();
        
        for(int i=ans.length-1;i>0;i-- )
        {
            str.append(ans[i]).append(" ");
        }
         str.append(ans[0]);
        
        return str.toString();
    }
}
